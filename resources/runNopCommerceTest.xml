<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="NopCommerce">

	<listeners>
		<listener class-name="commons.nopCommerce.MethodListener" />
		<!-- <listener class-name="reportConfig.nopCommerce.ReportNGListener" /> -->
		<listener class-name="reportConfig.nopCommerce.ExtentTestListenerV5" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="Run on Firefox">
		<parameter name="browser" value="Firefox" />
		<classes>
			<class name="com.nopcommerce.user.Level_15_ExtentReportV5_Screenshot" />
		</classes>
	</test>

